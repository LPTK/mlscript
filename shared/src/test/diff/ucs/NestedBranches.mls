fun optionApply(x, y, f) =
  if x is
    Some(xv) and y is
      Some(yv) then Some(f(xv, yv))
      None() then None()
    None() then None()
//│ optionApply: (None | Some, None | Some, (nothing, nothing,) -> 'value,) -> (None | Some & {value: 'value})
//│            = [Function: optionApply]



