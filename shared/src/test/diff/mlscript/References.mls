def swap a b = let tmp = a.Get in (a.Set b.Get, b.Set tmp)
//│ swap: Ref[in 'A | 'A0 out 'A0 & 'A] -> Ref[in 'A & 'A1 | 'A2 out 'A1 & ('A0 | 'A2)] -> (unit, unit,)
//│     = [Function: swap]

