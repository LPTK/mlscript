def eval_lexpr eval_rec subst v = case v of {
  | Lambda -> eval_lambda eval_rec subst v
  | Expr -> eval_expr eval_rec subst v
  }
//│ eval_lexpr: ((Cons[('a, Var | 'rhs,) | 'A]\head\tail & {head: ('a, Var | 'rhs,), tail: Nil | 'tail} | 'tail) -> 'rhs0 -> ('rhs & 'result & (Abs[?]\body\name & {body: 'rhs0, name: 'a} | 'lhs & ~#Abs))) -> (List['A] & 'b & 'c & 'tail) -> (Abs[?]\body\name & {body: 'rhs0, name: 'a} | Add[?] & {lhs: 'rhs0, rhs: 'rhs0} | App[?] & {lhs: 'rhs0, rhs: 'rhs0} | Mul[?] & {lhs: 'rhs0, rhs: 'rhs0} | Num & 'result | Var & 'result) -> (Abs['rhs] | Add['rhs] | App['lhs | 'rhs]\lhs\rhs & {lhs: 'lhs, rhs: 'rhs} | Mul['rhs] | Num | 'result)
