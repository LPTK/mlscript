
// https://github.com/cliffclick/aa/issues/28
pair x y = (x, y)
I = (fun x->x);  
K = (fun x->(fun y->x));  
W = (fun x->(x x));  
term = (fun z->(fun y ->((y (z I))(z K))));
test = (term W);
(test (fun x -> (fun y -> (pair (x 3) (((y "a") "b") "c")) ) ))
//│ pair: 'a -> 'b -> ('a, 'b,)
//│     = [Function: pair]
//│ I: 'a -> 'a
//│  = [Function: I]
//│ K: 'a -> anything -> 'a
//│  = [Function: K]
//│ W: (nothing -> 'a) -> 'a
//│  = [Function: W]
//│ term: ((forall 'a. 'a -> 'a) -> 'b & (forall 'c. 'c -> anything -> 'c) -> 'd) -> ('b -> 'd -> 'e) -> 'e
//│     = [Function: term]
//│ test: (nothing -> (anything -> nothing) -> 'a) -> 'a
//│     = [Function (anonymous)]
//│ res: (nothing, nothing,)
//│    = [ 3, 'b' ]

