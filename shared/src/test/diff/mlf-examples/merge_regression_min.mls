:DontGeneralizeCurriedFunctions


def choose: 'a -> 'a -> 'a
//│ choose: 'a -> 'a -> 'a
//│       = <missing implementation>

rec def id1 x = choose x (id1 id1 x)
//│ id1: 'a -> ('a | 'a -> nothing)
//│    = <no result>
//│      choose is not implemented

id1 id
//│ res: ('a & 'b) -> 'b | 'a
//│    = <no result>
//│      id1 and choose are not implemented


:GeneralizeCurriedFunctions

id1 id
//│ res: ('a & 'b) -> 'b | 'a
//│    = <no result>
//│      id1 and choose are not implemented

rec def id1 x = choose x (id1 id1 x)
//│ id1: 'a -> ('a | 'a -> nothing)
//│    = <no result>
//│      choose is not implemented

// :e
id1 id
//│ res: ('a & 'b) -> 'b | 'a
//│    = <no result>
//│      id1 and choose are not implemented

